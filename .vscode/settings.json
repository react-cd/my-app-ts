    "csscomb.supportEmbeddedStyles": false,
    "csscomb.syntaxAssociations": {},
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    // css 保存自动格式话
    "csscomb.formatOnSave": true,
    "editor.largeFileOptimizations": false,
    "gitlens.gitExplorer.files.layout": "list",
     "prettier.arrowParens": "always",
     // 在对象文字中的括号之间打印空格
     "prettier.bracketSpacing": true,
     // prettier
     "prettier.disableLanguages": [
         "vue"
     ],
     // "prettier.disableLanguages": [],
     "prettier.ignorePath": ".prettierignore",
     // "prettier.jsxBracketSameLine": false,
     // 指定使用哪个分析器
     "prettier.parser": "babylon",
     // 打印宽度指定打印将换行的行长
     "prettier.printWidth": 200,
     // 默认情况下，由于某些服务使用了对换行符敏感的呈现器，因此Prettier会按原样包装降价文本。
     "prettier.proseWrap": "preserve",
     // 是否启用项目的.prettierrc文件作为默认格式化文件
     "prettier.requireConfig": false,
     // 在语句的末尾打印分号
     "prettier.semi": false,
     // 使用单引号而不是双引号
     "prettier.singleQuote": true,
     "prettier.stylelintIntegration": false,
     // 指定每个缩进的空格数
     "prettier.tabWidth": 2,
     "prettier.trailingComma": "none",
     // 用Tab缩进而不是空格
     "prettier.useTabs": false
}